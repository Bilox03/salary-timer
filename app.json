{
  "expo": {
    "name": "Stipendio Timer",
    "slug": "stipendio-timer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "stipendiotimer",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bilox.stipendiotimer",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.bilox.stipendiotimer",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_nobg.png",
        "backgroundColor": "#1a1a1a"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_nobg.png",
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#1a1a1a",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
